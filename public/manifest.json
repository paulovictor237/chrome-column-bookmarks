{
  "name": "Column Bookmarks",
  "description": "Column Bookmarks",
  "version": "2.0.0",
  "version_name": "Beta",
  "manifest_version": 3,
  "permissions": ["bookmarks", "chrome://favicon/"],
  "browser_action": {
    "default_title": "Column",
    "default_icon": "/images/favicon96.png"
  },
  "icons": {
    "16": "/images/favicon.png",
    "32": "/images/favicon.png",
    "96": "/images/favicon.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
